{
  "include": [
    "./src/*",
    "src/Components/ActionControl",
    "src/ActionControlComponent/ActionControl.tsx",
    "src/Components/EventDispatcher/EventDispatcher.tsx",
    "src/Components/Flow/Flow.tsx"
  ],
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "jsx": "react-jsx",
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true
  }
}
